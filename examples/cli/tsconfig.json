{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "isolatedModules": false,
    "skipLibCheck": false,
    "baseUrl": ".",
    "jsx": "react",
    "paths": {
      "@/*": ["./*"]
    },
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
