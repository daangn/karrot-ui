{
  "compilerOptions": {
    "noImplicitAny": false,
    "target": "es6",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowJs": true,
    "declaration": true,
    "outDir": "lib",
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom", "dom.iterable"]
  },
  "include": ["src"]
}
