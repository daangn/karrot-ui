{
  "name": "@seed-design/recipe-generator-preset",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/seed-design.git",
    "directory": "packages/recipe-generator/preset"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    }
  },
  "main": "./lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "prepack": "rm -rf lib && yarn build",
    "generate": "yarn generate-vars ./src/__generated__",
    "build": "yarn build:style",
    "build:style": "yarn generate && nanobundle build"
  },
  "dependencies": {
    "@seed-design/recipe-generator-core": "0.0.0"
  },
  "devDependencies": {
    "@seed-design/component-spec-cli": "0.0.0",
    "nanobundle": "^1.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "ultra": {
    "concurrent": [
      "dev",
      "build"
    ]
  }
}
