{
  "name": "@seed-design/react-primitive",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/seed-design.git",
    "directory": "packages/react-primitive"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    }
  },
  "main": "./lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "prepack": "yarn build",
    "clean": "rm -rf lib",
    "build": "nanobundle build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "nanobundle": "^1.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "ultra": {
    "concurrent": [
      "build"
    ]
  }
}
