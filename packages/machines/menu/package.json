{
  "name": "@seed-design/menu",
  "version": "1.0.0-beta.0",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    }
  },
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "yarn build:js && yarn build:dts",
    "build:dts": "tsc --emitDeclarationOnly --noEmit false",
    "build:js": "node ./esbuild.config.js",
    "clean": "rimraf lib",
    "dev": "yarn build:js --watch && yarn build:dts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/seed-design.git",
    "directory": "packages/machines/button"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@seed-design/stylesheet": "workspace:^",
    "esbuild": "^0.15.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "ultra-runner": "^3.10.5"
  },
  "ultra": {
    "concurrent": [
      "dev",
      "build"
    ]
  },
  "dependencies": {
    "@zag-js/menu": "^0.1.13"
  }
}
