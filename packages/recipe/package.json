{
  "name": "@seed-design/recipe",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/seed-design.git",
    "directory": "packages/recipe"
  },
  "sideEffects": false,
  "exports": {
    "./*": {
      "types": "./lib/*.d.ts",
      "import": "./lib/*.mjs"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "prepack": "rm -rf lib && yarn build",
    "build:style": "yarn recipegen js ./lib"
  },
  "devDependencies": {
    "@seed-design/recipe-generator-cli": "0.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "ultra": {
    "concurrent": [
      "dev",
      "build"
    ]
  }
}
